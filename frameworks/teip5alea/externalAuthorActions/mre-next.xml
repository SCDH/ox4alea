<?xml version="1.0" encoding="UTF-8"?>
<!-- user action -->
<a:authorAction xmlns:a="http://www.oxygenxml.com/ns/author/external-action"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.oxygenxml.com/ns/author/external-action http://www.oxygenxml.com/ns/author/external-action/authorAction.xsd"
  id="mre.next">
  <a:name>MRE next</a:name>
  <a:description>Set the MRE: add the recension 'in progress' to the recensions in 'done' and shift to the next.</a:description>
  <a:smallIconPath href="${framework}/images/mre-next-16.png"/>
  <a:largeIconPath href="${framework}/images/mre-next-24.png"/>
  <a:operations>
    <a:operation id="ExecuteTransformationScenariosOperation">
      <a:xpathCondition>true()</a:xpathCondition>
      <a:arguments>
        <a:argument name="scenarioNames">ALEA :: MRE next</a:argument>
      </a:arguments>
    </a:operation>
  </a:operations>
  <a:accessKey/>
</a:authorAction>
