<?xml version="1.0" encoding="UTF-8"?>
<!-- editing atom -->
<a:authorAction xmlns:a="http://www.oxygenxml.com/ns/author/external-action"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.oxygenxml.com/ns/author/external-action http://www.oxygenxml.com/ns/author/external-action/authorAction.xsd"
  id="atom.move.caret.title">
  <a:name>Move</a:name>
  <a:description>Moves the caret to the title.</a:description>
  <a:operations>
    <a:operation id="MoveCaretOperation">
      <a:xpathCondition>descendant-or-self::title[@type eq 'lemma']</a:xpathCondition>
      <a:arguments>
        <a:argument name="position">Inside, at the beginning</a:argument>
        <a:argument name="xpathLocation">descendant-or-self::title[@type eq 'lemma']</a:argument>
      </a:arguments>
    </a:operation>
  </a:operations>
  <a:accessKey/>
</a:authorAction>
